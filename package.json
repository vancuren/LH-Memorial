{
  "name": "memorial",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cf:proxy": "npx cf-bindings-proxy --kv=vcgmemorialkv --r2=vcg-memorial-bucket",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static --project-name=lh-memorial",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --live-reload --compatibility-date=2023-11-21 --compatibility-flag=nodejs_compat -k=vcgmemorialkv --r2=vcg-memorial-bucket",
    "dev:linux": "npm run cf:proxy & npm run dev"
  },
  "dependencies": {
    "cf-bindings-proxy": "^0.7.1",
    "next": "14.0.3",
    "photoswipe": "^5.4.3",
    "react": "^18",
    "react-dom": "^18",
    "react-photoswipe-gallery": "^3.0.0-alpha.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.0",
    "@cloudflare/workers-types": "^4.20231121.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "eslint-plugin-next-on-pages": "^1.8.0",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vercel": "^32.6.1"
  }
}
